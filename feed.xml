<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:content="http://purl.org/rss/1.0/modules/content/" version="2.0">
  <channel>
    <title>Software Carpentry</title>
    <link>{{site.url}}</link>
    <description>Data is ones and zeroes. Software is ones and zeroes and hard work.</description>
    <copyright>Copyright (c) {{site.author}}.</copyright>
    <lastBuildDate>{{site.time | date: "%a, %d %b %Y %H:%M:%S %Z"}}</lastBuildDate>
    <generator>Emacs, Make, and Jekyll</generator>
    <docs>http://blogs.law.harvard.edu/tech/rss</docs>
    {% for post in site.posts limit:20 %}
      <item>
        <title>{{post.title}}</title>
        <author>{{site.email}} ({{site.author}})</author>
        <link>{{site.url}}{{post.url}}</link>
        <pubDate>{{post.date | date: "%a, %d %b %Y %H:%M:%S %Z"}}</pubDate>
        <description>{{post.content|xml_escape}}</description>
      </item>
    {% endfor %}
  </channel>
</rss>
